{
  "name": "api-server-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for API server.",
  "scripts": {
    "build": "NODE_ENV=production rm -rf build && babel --compact true --no-comments --minified -d build server.js && babel --compact true --no-comments --minified -d build/lib lib/",
    "lint": "eslint .",
    "nodemon": "nodemon server --exec babel-node",
    "prod": "NODE_ENV=production node build/server",
    "start": "babel-node server"
  },
  "author": "Alina Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "app-module-path": "^2.1.0",
    "express": "^4.14.0",
    "http-errors": "^1.5.1",
    "morgan": "^1.7.0",
    "statuses": "^1.3.1"
  }
}
